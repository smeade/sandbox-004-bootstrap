<div class="page-header">
  <%= link_to bootstrapped_posts_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_bootstrapped_post_path(@bootstrapped_post), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show bootstrapped_post</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @bootstrapped_post.title %></dd>

  <dt>Body:</dt>
  <dd><%= @bootstrapped_post.body %></dd>

  <dt>Published:</dt>
  <dd><%= @bootstrapped_post.published %></dd>

</dl>
